{
  "name": "composer",
  "version": "0.0.1",
  "description": "iPlug2 audio plug-in project manager",
  "main": "./dist/main.bundle.js",
  "scripts": {
    "start": "electron ./dist/main.bundle.js",
    "dev": "rimraf dist && cross-env NODE_ENV=development webpack --watch --progress --colors",
    "prod": "rimraf dist && cross-env NODE_ENV=production webpack",
    "test": "mocha -r ts-node/register -r tsconfig-paths/register \"test/**/*.ts\"",
    "coverage": "rimraf coverage && mkdir coverage && nyc npm run test && nyc report --reporter=text-lcov > coverage/coverage.txt",
    "build:win": "electron-builder build --win --x64 --publish never",
    "build:mac": "electron-builder build --mac --x64 --publish never",
    "lint": "eslint --max-warnings 0 --ext .js \"src/**/*.{ts,tsx}\" --fix"
  },
  "build": {
    "appId": "com.github.alexliesenfeld.composer",
    "productName": "Composer",
    "artifactName": "Composer-${version}.${ext}",
    "directories": {
      "app": "./dist/",
      "output": "./out/"
    },
    "mac": {
      "target": "pkg",
      "icon": "public/icon.png"
    },
    "win": {
      "target": "nsis",
      "asar": false,
      "icon": "public/icon.png"
    },
    "buildVersion": "1.0.0"
  },
  "author": "Alexander Liesenfeld",
  "license": "MIT",
  "dependencies": {
    "@blueprintjs/select": "^3.12.2",
    "@types/adm-zip": "^0.4.33",
    "@types/chokidar": "^2.1.3",
    "@types/css-font-loading-module": "0.0.4",
    "@types/fs-extra": "^9.0.0",
    "@types/request": "^2.48.4",
    "ace-builds": "^1.4.11",
    "adm-zip": "^0.4.14",
    "chokidar": "^3.4.0",
    "electron-sudo": "^4.0.12",
    "fs-extra": "^9.0.0",
    "mobx": "^5.15.4",
    "mobx-persist": "^0.4.1",
    "mobx-react": "^6.2.2",
    "npm-check-updates": "^4.1.2",
    "react": "^16.13.1",
    "react-ace": "^8.1.0",
    "react-dom": "^16.13.1",
    "react-hotkeys": "^2.0.0",
    "request": "^2.88.2",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "sudo-prompt": "^9.2.1"
  },
  "devDependencies": {
    "@blueprintjs/core": "^3.26.0",
    "@types/chai": "^4.2.11",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/mocha": "^7.0.2",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "acorn": "^7.1.1",
    "ajv": "^6.12.2",
    "chai": "^4.2.0",
    "copy-pkg-json-webpack-plugin": "0.0.38",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "electron": "^8.2.5",
    "electron-builder": "^22.6.0",
    "electron-devtools-installer": "^3.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "lodash": "^4.17.15",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "source-map-loader": "^0.2.4",
    "spectron": "^10.0.1",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.2",
    "ts-node": "^8.10.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alexliesenfeld/composer"
  },
  "keywords": [
    "boilerplate",
    "Electron",
    "React",
    "Typescript",
    "Webpack"
  ],
  "bugs": {
    "url": "https://github.com/alexliesenfeld/composer/issues"
  }
}
